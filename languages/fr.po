msgstr "Auto Bouclier"

msgctxt "scripts/text_weapons.lua_Science_Repulse_Upgrade2"
msgid "Shield Friendly"
msgstr "Bouclier Amical"

msgctxt "scripts/text_weapons.lua_Science_Repulse_A_UpgradeDescription"
msgid "Creates a personal Shield when used."
msgstr "Créer un bouclier personnel lorsqu'il est utilisé."

msgctxt "scripts/text_weapons.lua_Science_Repulse_B_UpgradeDescription"
msgid "Shield adjacent ally or Building."
msgstr "Applique une bouclier à un allié ou Bâtiment adjacent."

msgctxt "scripts/text_weapons.lua_Science_Swap_Name"
msgid "Teleporter"
msgstr "Téléporteur"

msgctxt "scripts/text_weapons.lua_Science_Swap_Description"
msgid "Swap places with a nearby tile."
msgstr "Échanger de place avec une tuile voisine."

msgctxt "scripts/text_weapons.lua_Science_Swap_Upgrade2"
msgid "+2 Range"
msgstr "+2 Distance"

msgctxt "scripts/text_weapons.lua_Science_Swap_A_UpgradeDescription"
msgid "Extends range of the swap by 1 tile."
msgstr "Étend la distance de l'échange de 1 tuile."

msgctxt "scripts/text_weapons.lua_Science_Swap_B_UpgradeDescription"
msgid "Extends range of the swap by 2 tiles."
msgstr "Étend la distance de l'échange de 2 tuiles."

msgctxt "scripts/text_weapons.lua_Science_AcidShot_Name"
msgid "A.C.I.D. Projector"
msgstr "Projecteur A.C.I.D."

msgctxt "scripts/text_weapons.lua_Science_AcidShot_Description"
msgid "Fire a projectile that applies A.C.I.D. and pushes."
msgstr "Tirer un projectile qui applique A.C.I.D. et Pousse."

msgctxt "scripts/text_weapons.lua_Science_Confuse_Name"
msgid "Confuse Shot"
msgstr "Tir Confus"

msgctxt "scripts/text_weapons.lua_Science_Confuse_Description"
msgid "Fire a projectile that flips a target's attack direction."
msgstr "Tirez un projectile qui retourne la direction d'attaque d'une cible."

msgctxt "scripts/text_weapons.lua_Science_SmokeDefense_Name"
msgid "Smoke Pellets"
msgstr "Fumigène"

msgctxt "scripts/text_weapons.lua_Science_SmokeDefense_Description"
msgid "Surround yourself with Smoke to defend against nearby enemies."
msgstr ""
"Entoure de fumée votre unité pour vous défendre contre les ennemis proches."

msgctxt "scripts/text_weapons.lua_Science_SmokeDefense_A_UpgradeDescription"
msgid "No longer applies Smoke on self or allies."
msgstr "N'applique plus Fumée sur soi-même ou ses alliés."

msgctxt "scripts/text_weapons.lua_Science_Shield_Name"
msgid "Shield Projector"
msgstr "Bouclier Projecteur"

msgctxt "scripts/text_weapons.lua_Science_Shield_Description"
msgid "Shield tiles from damage."
msgstr "Protège les tuiles des dégâts."

msgctxt "scripts/text_weapons.lua_Science_Shield_Upgrade2"
msgid "+3 Area"
msgstr "+3 Zone"

msgctxt "scripts/text_weapons.lua_Science_Shield_B_UpgradeDescription"
msgid "Shields 3 additional tiles."
msgstr "Protège 3 tuiles supplémentaires."

msgctxt "scripts/text_weapons.lua_Science_FireBeam_Name"
msgid "Fire Beam"
msgstr "Rayon Enflammé"

msgctxt "scripts/text_weapons.lua_Science_FireBeam_Description"
msgid "Fire a beam that applies Fire in a line."
msgstr "Tirer un rayon qui applique le Feu sur une ligne."

msgctxt "scripts/text_weapons.lua_Science_FreezeBeam_Name"
msgid "Frost Beam"
msgstr "Rayon Gelé"

msgctxt "scripts/text_weapons.lua_Science_FreezeBeam_Description"
msgid "Fire a beam that Freezes everything in a line."
msgstr "Tirer un rayon qui Gèle tout sur une ligne."

msgctxt "scripts/text_weapons.lua_Science_LocalShield_Name"
msgid "Shield Array"
msgstr "Boucliers Zone"

msgctxt "scripts/text_weapons.lua_Science_LocalShield_Description"
msgid "Apply a Shield on nearby tiles."
msgstr "Applique un bouclier sur les tuiles voisines."

msgctxt ""
"scripts/text_weapons.lua_Science_LocalShield_Upgrade1,scripts/text_weapons."
"lua_Support_Blizzard_Upgrade1,scripts/text_weapons."
"lua_Support_Blizzard_Upgrade2"
msgid "+1 Size"
msgstr "+1 Taille"

msgctxt "scripts/text_weapons.lua_Science_LocalShield_A_UpgradeDescription"
msgid "Area of effect increased by 2 tiles."
msgstr "Zone d'effet augmentée de 2 tuiles."

msgctxt "scripts/text_weapons.lua_Science_PushBeam_Name"
msgid "Push Beam"
msgstr "Rayon Pousseur"

msgctxt "scripts/text_weapons.lua_Science_PushBeam_Description"
msgid "Pushes all units in a line."
msgstr "Pousse toutes les unités dans une ligne."

msgctxt "scripts/text_weapons.lua_Science_PushBeam_Upgrade1"
msgid "Unlimited Use"
msgstr "Utilisation Illimitée"

msgctxt ""
"scripts/text_weapons.lua_Science_PushBeam_A_UpgradeDescription,scripts/"
"text_weapons.lua_Support_Wind_A_UpgradeDescription"
msgid "Removes use restriction in battles."
msgstr "Supprime la restriction d'utilisation dans les batailles."

msgctxt "scripts/text_weapons.lua_Skill_Repair_Name"
msgid "Mech Repair"
msgstr "Réparation Mech"

msgctxt "scripts/text_weapons.lua_Skill_Repair_Description"
msgid "Repair 1 damage and remove Fire, Ice, and A.C.I.D."
msgstr "Réparer 1 de dégât et retirer l'effet de Feu, de Glace et d'A.C.I.D."

msgctxt "scripts/text_weapons.lua_Skill_Repair_Power_Name"
msgid "Power Repair"
msgstr "Réparation Puissante"

msgctxt "scripts/text_weapons.lua_Skill_Repair_Power_Description"
msgid "Repair Mech and push adjacent tiles."
msgstr "Réparer le Mech et pousser les tuiles adjacents."

msgctxt "scripts/text_weapons.lua_Skill_Repair_Punch_Name"
msgid "Mantis Slash"
msgstr "Mantis Entailleur"

msgctxt "scripts/text_weapons.lua_Skill_Repair_Punch_Description"
msgid ""
"Damage and push an adjacent tile.\n"
"Escape from Ice."
msgstr ""
"Afflige des Dégâts et Pousse une tuile adjacente.\n"
"Se libère de la glace."

msgctxt "scripts/text_weapons.lua_Support_Boosters_Name"
msgid "Boosters"
msgstr "Amplificateurs"

msgctxt "scripts/text_weapons.lua_Support_Boosters_Description"
msgid "Jump forward and push adjacent tiles away."
msgstr "Sauter en avant et Pousse les carreaux adjacents."

msgctxt "scripts/text_weapons.lua_Support_Smoke_Name"
msgid "Smoke Bombs"
msgstr "Bombes Fumigènes"

msgctxt "scripts/text_weapons.lua_Support_Smoke_Description"
msgid "Fly over the targets while dropping Smoke."
msgstr "Survoler les cibles tout en laissant tomber de la Fumée."

msgctxt ""
"scripts/text_weapons.lua_Support_Smoke_A_UpgradeDescription,scripts/"
"text_weapons.lua_Support_Smoke_B_UpgradeDescription"
msgid "Increases potential movement range by 1."
msgstr "Augmente le Déplacement potentielle de 1."

msgctxt "scripts/text_weapons.lua_Support_Refrigerate_Name"
msgid "Heat Converter"
msgstr "Convertisseur Thermique"

msgctxt "scripts/text_weapons.lua_Support_Refrigerate_Description"
msgid ""
"Freeze the tile in front but light the tile behind on Fire in the process."
msgstr "Gèle la tuile devant, mais Enflamme celle derrière dans le processus."

msgctxt "scripts/text_weapons.lua_Support_Destruct_Name"
msgid "Self-Destruct"
msgstr "Auto-Destruction"

msgctxt "scripts/text_weapons.lua_Support_Destruct_Description"
msgid "Mech explodes, killing self and anything adjacent to Mech."
msgstr "Le Mech explose, se tue ainsi que tout ce qui est adjacent à ce Mech."

msgctxt "scripts/text_weapons.lua_Support_Force_Name"
msgid "Targeted Strike"
msgstr "Frappe Ciblée"

msgctxt "scripts/text_weapons.lua_Support_Force_Description"
msgid "Call in an air strike on a single tile anywhere on the map."
msgstr ""
"Appeler une frappe aérienne sur une seule tuile n'importe où sur la carte."

msgctxt "scripts/text_weapons.lua_Support_SmokeDrop_Name"
msgid "Smoke Drop"
msgstr "Largage Fumée"

msgctxt "scripts/text_weapons.lua_Support_SmokeDrop_Description"
msgid "Drops Smoke on 5 tiles anywhere on the map."
msgstr "Largage Fumée sur 5 tuiles n'importe où sur la carte."

msgctxt "scripts/text_weapons.lua_Support_Repair_Name"
msgid "Repair Drop"
msgstr "Larger Réparation"

msgctxt "scripts/text_weapons.lua_Support_Repair_Description"
msgid "Heal all player units (including disabled Mechs)."
msgstr "Soigne toutes les unités joueurs (y compris les Mechs désactivés)."

msgctxt "scripts/text_weapons.lua_Support_Missiles_Name"
msgid "Missile Barrage"
msgstr "Barrage de Missile"

msgctxt "scripts/text_weapons.lua_Support_Missiles_Description"
msgid "Fires a missile barrage that hits every enemy on the map."
msgstr "Tire un barrage de missiles qui frappe tous les ennemis sur la carte."

msgctxt "scripts/text_weapons.lua_Support_Wind_Name"
msgid "Wind Torrent"
msgstr "Vent Tournoyant"

msgctxt "scripts/text_weapons.lua_Support_Wind_Description"
msgid "Push all units in a single direction."
msgstr "Pousser toutes les unités dans une seul direction."

msgctxt "scripts/text_weapons.lua_Support_Wind_Upgrade1"
msgid "Unlimited Uses"
msgstr "Utilisations Illimitées"

msgctxt "scripts/text_weapons.lua_Support_Blizzard_Name"
msgid "Ice Generator"
msgstr "Générateur Glacé"

msgctxt "scripts/text_weapons.lua_Support_Blizzard_Description"
msgid "Freeze yourself and nearby tiles."
msgstr "Vous Gèle ainsi que les tuiles adjacentes."

msgctxt ""
"scripts/text_weapons.lua_Support_Blizzard_A_UpgradeDescription,scripts/"
"text_weapons.lua_Support_Blizzard_B_UpgradeDescription"
msgid "Area of effect increased by 1 tile."
msgstr "Zone d'effet augmentée de 1 tuile."

msgctxt ""
"scripts/text_weapons.lua_Deploy_TankShot_Name,scripts/text_weapons."
"lua_Deploy_TankShot2_Name"
msgid "Stock Cannon"
msgstr "Canon Réservoir"

