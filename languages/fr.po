"mission_acidtank.lua#3"
msgid "A.C.I.D. Tank"
msgstr "Char A.C.I.D"

msgctxt "scripts/text_weapons.lua_DeploySkill_AcidTank_Description"
msgid "Deploy a Tank that can apply A.C.I.D. to targets."
msgstr "Déployer un Char qui peut appliquer A.C.I.D aux cibles."

msgctxt "scripts/text_weapons.lua_DeploySkill_AcidTank_Upgrade2"
msgid "+Push"
msgstr "+Pousser"

msgctxt "scripts/text_weapons.lua_DeploySkill_AcidTank_B_UpgradeDescription"
