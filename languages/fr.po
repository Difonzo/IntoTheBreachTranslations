msgstr "L'attaque du char pousse également ces cibles."

msgctxt "scripts/text_weapons.lua_Deploy_PullTankShot_Name"
msgid "Pull Shot"
msgstr "Tir Attirant"

msgctxt "scripts/text_weapons.lua_Deploy_PullTankShot_Description"
msgid "Pulls a single tile."
msgstr "Attire sur une seul tuile."

msgctxt "scripts/text_weapons.lua_DeploySkill_PullTank_Name"
msgid "Pull-Tank"
msgstr "Char-Attirant"

msgctxt "scripts/text_weapons.lua_DeploySkill_PullTank_Description"
msgid "Deploy a Pull-Tank that can pull targets with a projectile."
msgstr ""
"Déployer un Char-Attirant qui peut déplacer les cibles avec un projectile."

msgctxt "scripts/text_weapons.lua_DeploySkill_PullTank_A_UpgradeDescription"
msgid "Increases the Pull-Tank's max health to 3."
msgstr "Augmente la santé max du Char-Attirant à 3."

msgctxt "scripts/text_weapons.lua_DeploySkill_PullTank_B_UpgradeDescription"
msgid "Gives the Pull-Tank the ability to fly."
msgstr "Donne au Char-Attirant la capacité de voler."

msgctxt "scripts/text_weapons.lua_DeploySkill_SGenerator_Description"
msgid "Deploy a Shield-Tank that can give shields to allies."
msgstr "Déployer un Char-Bouclier qui peut donner des boucliers aux alliés."

msgctxt "scripts/text_weapons.lua_DeploySkill_SGenerator_A_UpgradeDescription"
