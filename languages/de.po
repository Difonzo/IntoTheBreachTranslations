msgid "Turns into Water when destroyed. Must be hit twice."
msgstr "Wird bei Zerstörung in Wasser verwandelt. Muss zweimal getroffen werden."

msgctxt "scripts/text_tooltips.lua_TILE_TOOLTIPS__acid_ice_0"
msgid "A.C.I.D. Ice"
msgstr "Säureeis"

msgctxt "scripts/text_tooltips.lua_TILE_TOOLTIPS__acid_ice_1"
