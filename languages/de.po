msgctxt "scripts/text_weapons.lua_Brute_Bombrun_B_UpgradeDescription"
msgid "Increases damage to all tiles by 2."
msgstr "Erhöht den Schaden an allen Feldern um 2."

msgctxt "scripts/text_weapons.lua_Brute_Sonic_Name"
msgid "Hermes Engines"
msgstr "Hermes-Motoren"

msgctxt "scripts/text_weapons.lua_Brute_Sonic_Description"
msgid "Dash in a line, pushing adjacent tiles away."
msgstr "Schiesse in eine Linie und stoße angrenzende Felder​​ab."

msgctxt "scripts/text_weapons.lua_Ranged_Artillerymech_Name"
